{
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "name": "Mosh Client",
  "description": "Mosh Client",
  "offline_enabled": true,
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"socket": [
      "udp-bind",
      "udp-send-to"
    ]},
  "clipboardRead",
  "clipboardWrite",
  "storage"
  ]
}
